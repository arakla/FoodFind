<div id="map_canvas"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map_canvas'), {
      center: {lat: 40.443, lng: -79.9435},
      zoom: 17,
      disableDefaultUI: true
    });

    var marker = new google.maps.Marker({
      position: {lat: 40.441802, lng: -79.946229},
      label: "3",
      map: map
    });
  }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCMOiRIvvh1MmUZGchc5wJZEY68gjnKtwY&callback=initMap"
async defer></script>
